@using System.Data
@{
    DataTable dt = new DataTable();
    var columns = new List<string>();
    if (Model != null)
    {
        dt = Model;
        foreach (DataColumn dc in dt.Columns)
        {
            columns.Add(dc.ColumnName);
        }
        var ex = new List<string>() { "", "", "", "", "", "" };
        columns = columns.Where(x => !ex.Any(y => y == x)).ToList();
    }
}
<style>
    table.dataTable thead > tr > th.sorting, table.dataTable thead > tr > th.sorting_asc, table.dataTable thead > tr > th.sorting_desc, table.dataTable thead > tr > th.sorting_asc_disabled, table.dataTable thead > tr > th.sorting_desc_disabled, table.dataTable thead > tr > td.sorting, table.dataTable thead > tr > td.sorting_asc, table.dataTable thead > tr > td.sorting_desc, table.dataTable thead > tr > td.sorting_asc_disabled, table.dataTable thead > tr > td.sorting_desc_disabled {
        /*padding-right: 0px !important;*/
    }
</style>
<h5 class="text-center"><strong class="text-bg-primary">@dt.Rows[0]["RoundType"]</strong></h5>
<div class="bg-light rounded h-100 p-4">
    <div class="table-responsive">
        <table class="table table-bordered table-striped cell-border small" id="tbl">
            <thead class="text-center">
                <tr>
                    <th class="text-center">Srno.</th>
                    <th class="text-center">District</th>
                    @if (ViewBag.DistrictBlockType == "2")
                    {
                        <th class="text-center">Block</th>
                    }
                    <th class="text-center">Indicator</th>
                    <th class="text-center">Total</th>
                    <th class="text-center">Male</th>
                    <th class="text-center">Female</th>
                </tr>
            </thead>
            <tbody>
                @{
                    if (dt.Rows.Count > 0)
                    {
                        var sno = 1;
                        foreach (DataRow dr in dt.Rows)
                        {
                            <tr>
                                <td class="text-center">@(sno++)</td>
                                <td class="text-center">@dr["district"]</td>
                                @if (ViewBag.DistrictBlockType == "2")
                                {
                                    <td class="text-center">@dr["block"]</td>
                                }
                                <td class="text-center">@dr["indicatorText"]</td>
                                <td class="text-center">@dr["total"]</td>
                                <td class="text-center">@dr["male"]</td>
                                <td class="text-center">@dr["female"]</td>
                            </tr>
                        }
                    }
                    else
                    {
                        <tr>
                            <td colspan="@columns.Count"><strong class="text-danger">Record Not Found !!</strong></td>
                        </tr>
                    }
                }
            </tbody>

        </table>
    </div>
</div>

<script>
    $(function () {
        $("#tbl").DataTable();
    });
</script>
